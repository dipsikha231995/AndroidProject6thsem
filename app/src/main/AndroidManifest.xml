<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.applicationformcv">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="e-PANJEEYAN"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.DarkActionBar">
        <activity android:name=".AppointmentConfirmed"
            android:parentActivityName=".DeedRegistration"/>
        <activity
            android:name=".ViewStatus"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Groomdetails"
            android:parentActivityName=".Bridedetails" />
        <activity
            android:name=".Bridedetails"
            android:parentActivityName=".MarriageRegistration" />
        <activity
            android:name=".MarriageRegistration"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DeedRegistration"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>